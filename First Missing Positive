class Solution:
    def firstMissingPositive(self, nums: List[int]) -> int:
        l = len(nums)
        i=0
        while(i<l):
            correct_idx =nums[i]-1
            if nums[i]>=1 and nums[i]<=l and nums[i]!=nums[correct_idx]:
                nums[i], nums[correct_idx] = nums[correct_idx], nums[i]
            else:
                i+=1
        for i in range(l):
            if nums[i]!=i+1:
                return i+1
        return l+1
        
            
