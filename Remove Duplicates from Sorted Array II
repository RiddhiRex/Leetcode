class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        i = 0
        for n in nums:
            print(i, n, nums[i-2])
            if i < 2 or n > nums[i-2]:
                nums[i] = n
                i += 1
        return i


def removeDuplicates(nums):
    if len(nums) <= 2:
        return len(nums)

    slow = 2
    for fast in range(2, len(nums)):
        if nums[slow - 2] != nums[fast]:
            nums[slow] = nums[fast]
            slow += 1

    return slow
